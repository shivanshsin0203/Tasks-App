{
  "expo": {
    "name": "ToDoApp",
    "slug": "ToDoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/task.png",
    "scheme": "ToDoApp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logos.png",
      "resizeMode": "contain",
      "backgroundColor": "#374151"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/task.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    }
    
  }
}
